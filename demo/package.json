{
  "name": "gatsbylius-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsbylius",
  "version": "0.1.0",
  "author": "Waiting for a contact email, see https://github.com/Gatsbylius/Gatsbylius",
  "dependencies": {
    "gatsby": "^2.19.12",
    "gatsby-theme-sylius": "^0.1.0"
  },
  "devDependencies": {
    "cypress": "^3.8.2",
    "gh-pages": "^2.1.1",
    "start-server-and-test": "^1.10.8"
  },
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "clean": "gatsby clean",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open",
    "test:e2e:ci": "start-server-and-test develop http://localhost:8000 cy:run"
  }
}
